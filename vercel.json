{
  "functions": {
    "pages/api/**/*.js": {
      "maxDuration": 10
    }
  },
  "outputFileTracing": false,
  "headers": [
    {
      "source": "/audio/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/audio/(.*)",
      "destination": "/audio/$1"
    }
  ]
}
